{
  "name": "redux-automata",
  "version": "0.0.0",
  "description": "Finite state automata for Redux.",
  "main": "lib/redux-automata.js",
  "unpkg": "dist/redux-automata.js",
  "module": "es/redux-automata.js",
  "scripts": {
    "clean": "rimraf lib dist es coverage types",
    "build": "rollup -c",
    "lint": "eslint --ext ts src test",
    "test": "jest --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mocoding-software/redux-automata.git"
  },
  "keywords": [
